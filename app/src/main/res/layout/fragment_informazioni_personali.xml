        <View
            android:id="@+id/lineRectangle11"
            android:layout_width="@dimen/_311pxh"
            android:layout_height="@dimen/_1pxv"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="@dimen/_16pxv"
